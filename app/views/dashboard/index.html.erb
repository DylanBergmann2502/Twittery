<div class="col-6">
  <div class="mb-3">
    <%= link_to 'Home', dashboard_path, class: "fs-4 text-decoration-none text-black fw-semibold" %>
  </div>
  <%= form_with model: Tweet.new, url: tweets_path, local: true do |f| %>
    <%= f.text_area :body, placeholder: "What's happening?", class: "form-control w-100 tweet-input mb-4" %>
    <div class="d-flex justify-content-end">
      <%= f.submit "Tweet", class: "btn btn-twitter btn-lg rounded-pill" %>
    </div>
  <% end %>
  <ul>
    <%= Tweet.find_each do |tweet| %>
      <li>
        <%= tweet.user.username %>:
        <%= tweet.body %>
      </li>
    <% end %>
  </ul>
</div>
<div class="col">
  One of three columns
</div>




